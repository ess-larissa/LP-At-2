def eh_primo(num):
    if num <= 1:
        return False
    for i in range(2, int(num ** 0.5) + 1):
        if num % i == 0:
            return False
    return True

# Testando números de 1 a 100
for numero in range(1, 101):
    if eh_primo(numero):
        print(f'{numero} é primo')
    else:
        print(f'{numero} não é primo')
